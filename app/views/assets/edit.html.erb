<div id="wrapper">

  <%= render 'shared/sidebar' %>

  <div id="content-wrapper">
    <div class="container-fluid">
      <div class="row">
        <div class="col">
          <nav aria-label="breadcrumb">
            <ol class="breadcrumb bg-white breadcrumb-right-arrow">
              <li class="breadcrumb-item"><%= link_to "Mis archivos", files_path %></li>
              <li class="breadcrumb-item active" aria-current="page">Renombrar archivo</li>
            </ol>
          </nav>
        </div>
      </div>
      <div class="row">
        <div class="col-md-6 col-lg-7 col-xl-8 mx-auto">
          <div class="card">
            <div class="card-body">
              <%= render 'form', asset: @asset %>
            </div>
          </div>
          <%= render 'back_link' %>
        </div>
      </div>
      <br>
    </div>
    <br>
    <%= render 'shared/panel_footer' %>
  </div>

</div>
