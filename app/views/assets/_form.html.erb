<%= form_for @asset, :html => {:multipart => true} do |f| %>
  <div class="form-group">
    <div class="custom-file">
        <%= f.file_field :uploaded_file, class: "custom-file-input", id: "customFile" %>
        <%= f.label :uploaded_file, "Seleccionar archivo", class: "custom-file-label", for: "customFile" %><br />
    </div>
  </div>
  <p><%= f.submit "Subir", class: "btn btn-block btn-primary" %></p>
<% end %>
